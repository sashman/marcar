datasource db {
  provider = "postgresql"
  url = "***"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @default(autoincrement()) @id
  name  String
  email String @unique
  post Post[]
}

model Post {
  id  Int    @default(autoincrement()) @id
  name String
}